\subsubsection*{Linear regression in R}
\label{sec:orgc495b46}

\begin{verbatim}
x <- mtcars$wt
y <- mtcars$mpg
plot(x,y,xlab="Weight",ylab="Miles per gallon")
lm_model <- lm(y~x,data=mtcars)
abline(lm_model)
\end{verbatim}

